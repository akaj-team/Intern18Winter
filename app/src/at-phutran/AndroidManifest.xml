<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="asiantech.internship.summer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewandviewgroup.ListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewandviewgroup.ViewActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".eventlistener.SignUpActivity"
            android:label="@string/title" />
        <activity android:name=".fragment.LoginActivity" />
        <activity android:name=".fragment.WelcomeActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity" />
        <activity android:name=".viewpager.ViewPagerActivity" />
        <activity
            android:name=".canvas.CanvasActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".asynctask.AsyncTaskThreadHandlerActivity" />
        <activity android:name=".unittest.UnitTestActivity" />
    </application>
</manifest>
