<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="asiantech.internship.summer.canvas.CanvasActivity">
    <asiantech.internship.summer.canvas.GraphicViewSale
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:colorOfLine="@color/colorLineGray"
        app:colorOfSale="@color/colorColumnBlue"
        app:colorOfExpense="@color/colorColumnOrange"
        app:colorOfBackground="@color/colorWhite"
        app:colorOfText="@color/colorTextBlack"
        app:widthOfColumn="@dimen/widthColumn"
        app:widthOfNoteColumn="@dimen/widthNoteColumn"
        app:textNote="@dimen/textNote"
        app:textContent="@dimen/textContent"/>
</FrameLayout>
